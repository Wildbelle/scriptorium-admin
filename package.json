{
  "name": "scriptorium",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "proxy": "local-ssl-proxy --key ./ssl/localhost-key.pem --cert ./ssl/localhost.pem --source 3001 --target 3000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "./node_modules/.bin/prettier --write",
      "./node_modules/.bin/eslint --fix"
    ]
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.15",
    "@mui/material": "^5.10.15",
    "@mui/styled-engine-sc": "^5.10.14",
    "@next/font": "^13.0.6",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.1",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.2",
    "formik": "^2.2.9",
    "gsap": "^3.11.3",
    "next": "13.0.2",
    "next-redux-wrapper": "^8.0.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-locomotive-scroll": "^0.2.2",
    "react-redux": "^8.0.5",
    "styled-components": "^5.3.6",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "local-ssl-proxy": "^1.3.0",
    "prettier": "^2.7.1"
  }
}
